<h1>{{ title }}</h1>
<div>
    <button onclick="location.href='/select2'">Select</button >
</div>
<br>
<ul>
    {{#each student}}
    <li>ID: {{S_ID}}</li>
    <li>Name: {{S_Name}}</li>
    <li>Email: {{Email}}</li>
    <li>Phone: {{Phone}}</li>
    <li>Major: {{Major}}</li>
    {{/each}}
</ul>

<h1>{{ title2 }}</h1>
<table>
    <tr>
        <td>ID</td>
        <td>Name</td>
        <td>Participants</td>
        <td>Professor</td>
        <td>Building</td>
        <td>Room</td>
        <td>Department</td>
    </tr>
    {{#each clas}}
        <form method="post">
            <tr>
                <td>{{C_ID}}</td>
                <td>{{C_Name}}</td>
                <td>{{Participants}}</td>
                <td>{{Professor}}</td>
                <td>{{B_Name}}</td>
                <td>{{R_Name}}</td>
                <td>{{D_Name}}</td>
                <td style="border: none; margin-left: 10px;">
                    <button
                        style="margin-left: 10px;"
                        name='delBtn'
                        type="submit"
                        value="{{C_ID}}"
                        formaction="/student/delete">
                        삭제
                    </button>
                </td>
            </tr>
        </form>
    {{/each}}
</table>